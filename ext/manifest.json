{
  "manifest_version": 2,
  "name": "nitrio ext",
  "description": "fun",
  "version": "0.0.1",
  "browser_action": {
    //    "default_icon": "hustler.png",
    "default_popup": "popup.html",
    "default_title": "Article Hustler"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js",
        "jquery.min.js"
      ],
      "css": [
        "w3.css"
      ]
    }
  ],
  "permissions": [
    "contextMenus",
    "tabs",
    "cookies"
  ],
  "background": {
    "scripts": [
      "hustle.js",
      "jquery.min.js"
    ]
  }
}
